# Frontend environment variables
# Set VITE_USE_FAKES to 'true' to use local fake data (default).
# Set to 'false' to make the frontend call the backend API at /clients.
VITE_USE_FAKES=false

# Optional: override API base URL used by axios boot (src/boot/axios.js)
# VITE_API_BASE_URL=http://localhost:3333

# Toggle compare debug
DEBUG_COMPARE=false
VITE_DEBUG_COMPARE=false

# Enable verbose debug logs from composables when set to 'true'
# Set to 'true' only in development. Default is false.
VITE_DEBUG=true
